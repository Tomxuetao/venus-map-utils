{
  "name": "map-venus",
  "private": false,
  "version": "0.1.1",
  "type": "module",
  "files": [
    "dist"
  ],
  "license": "ISC",
  "typings": "./dist/index.d.ts",
  "main": "./dist/map-venus.umd.cjs",
  "module": "./dist/map-venus.js",
  "exports": {
    ".": {
      "import": "./dist/map-venus.js",
      "require": "./dist/map-venus.umd.cjs"
    }
  },
  "scripts": {
    "build": "tsc && vite build",
    "changeset": "changeset",
    "prepare": "husky install",
    "version": "changeset version",
    "commit": "git add . && git-cz",
    "preinstall": "npx only-allow pnpm",
    "fix": "eslint --ext .js,.ts src --fix"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.1",
    "@commitlint/cli": "^18.6.1",
    "@commitlint/config-conventional": "^18.6.3",
    "@types/node": "^20.12.7",
    "commitizen": "^4.3.0",
    "eslint": "^8.57.0",
    "git-cz": "^4.9.0",
    "husky": "^8.0.3",
    "nano-staged": "^0.8.0",
    "typescript": "^5.4.5",
    "vite": "^5.2.10",
    "vite-plugin-dts": "^3.9.0"
  },
  "config": {
    "commitizen": {
      "path": "git-cz"
    }
  },
  "lint-staged": {
    "*.{vue,js,ts}": "eslint --fix"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "engines": {
    "pnpm": ">= 8.8.0"
  },
  "nano-staged": {
    "*.{ts,tsx,js,vue}": "eslint --fix"
  },
  "peerDependencies": {
    "ol": "^9.1.0"
  }
}
