{
  "name": "map-venus",
  "private": false,
  "version": "0.1.8",
  "type": "module",
  "files": [
    "dist"
  ],
  "license": "ISC",
  "typings": "./dist/index.d.ts",
  "main": "./dist/map-venus.umd.cjs",
  "module": "./dist/map-venus.js",
  "exports": {
    ".": {
      "import": "./dist/map-venus.js",
      "require": "./dist/map-venus.umd.cjs"
    }
  },
  "scripts": {
    "build": "tsc && vite build",
    "changeset": "changeset",
    "prepare": "husky",
    "fix": "eslint . --fix",
    "version": "changeset version",
    "commit": "git add . && git-cz",
    "preinstall": "npx only-allow pnpm"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.12",
    "@commitlint/cli": "^19.7.1",
    "@commitlint/config-conventional": "^19.7.1",
    "@types/node": "^22.13.1",
    "commitizen": "^4.3.1",
    "eslint": "^9.19.0",
    "git-cz": "^4.9.0",
    "husky": "^9.1.7",
    "nano-staged": "^0.8.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.23.0",
    "vite": "^6.1.0",
    "vite-plugin-dts": "^4.5.0"
  },
  "config": {
    "commitizen": {
      "path": "git-cz"
    }
  },
  "lint-staged": {
    "*.{ts}": "eslint --fix"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "engines": {
    "pnpm": ">= 8.8.0"
  },
  "nano-staged": {
    "*.{ts}": "eslint --fix"
  },
  "peerDependencies": {
    "ol": "^9.2.4"
  }
}
