{
  "name": "venus-map-utils",
  "license": "ISC",
  "type": "module",
  "scripts": {
    "prepare": "husky",
    "changeset": "changeset",
    "version": "changeset version",
    "commit": "git add . && git-cz",
    "preinstall": "npx only-allow pnpm",
    "dev": "pnpm --dir packages/example dev",
    "build:ol": "pnpm --dir packages/map-venus build",
    "build": "pnpm -r --filter='./packages/*' run build",
    "build:amap": "pnpm --dir packages/venus-amap build",
    "build:example": "pnpm --dir packages/example build",
    "lint": "eslint ./packages/**/src --ext .js,.ts,.tsx,.mjs,.cjs --fix"
  },
  "devDependencies": {
    "@changesets/changelog-git": "^0.2.1",
    "@changesets/cli": "^2.28.1",
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@eslint/js": "^9.24.0",
    "@types/eslint-config-prettier": "^6.11.3",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^22.14.0",
    "@types/serve-static": "^1.15.7",
    "@typescript-eslint/eslint-plugin": "^8.29.1",
    "@typescript-eslint/parser": "^8.29.1",
    "eslint": "^9.24.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.6",
    "git-cz": "^4.9.0",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "nano-staged": "^0.8.0",
    "prettier": "^3.5.3",
    "rollup-plugin-external-globals": "^0.13.0",
    "tsup": "^8.4.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.29.1"
  },
  "engines": {
    "node": "^18.0.0 || ^20.0.0 || >=22.0.0"
  },
  "packageManager": "pnpm@10.5.2",
  "nano-staged": {
    "*.{ts,tsx,js,vue}": "eslint --fix",
    "*.{ts,tsx,js,vue,scss}": "prettier --write"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
