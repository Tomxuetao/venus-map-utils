{
  "name": "map-venus",
  "private": false,
  "version": "0.2.0",
  "files": [
    "dist"
  ],
  "license": "ISC",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "changeset": "changeset",
    "prepare": "husky",
    "fix": "eslint . --fix",
    "version": "changeset version",
    "commit": "git add . && git-cz",
    "preinstall": "npx only-allow pnpm"
  },
  "devDependencies": {
    "@changesets/cli": "^2.28.1",
    "@commitlint/cli": "^19.7.1",
    "@commitlint/config-conventional": "^19.7.1",
    "@types/node": "^22.13.5",
    "commitizen": "^4.3.1",
    "eslint": "^9.21.0",
    "git-cz": "^4.9.0",
    "husky": "^9.1.7",
    "tsup": "^8.3.6",
    "nano-staged": "^0.8.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.25.0",
    "rollup-plugin-external-globals": "^0.13.0"
  },
  "config": {
    "commitizen": {
      "path": "git-cz"
    }
  },
  "lint-staged": {
    "*.{ts}": "eslint --fix"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "engines": {
    "pnpm": ">= 8.8.0"
  },
  "nano-staged": {
    "*.{ts}": "eslint --fix"
  },
  "peerDependencies": {
    "ol": ">=10.4.0"
  }
}
